      {{else}}
      <option value="{{this.class}}" data-label="{{#with (or this.label this.name) }}{{{this}}}{{/with}}" data-class="{{this.class}}" data-label-type="labels" {{#if this.label-only }}hidden{{/if}}>{{this.name}}</option>
      {{/if}}
      {{/each}}
    </select>
  </div>
</div>
{{/if}} --}}

</div>
{{/if}}


