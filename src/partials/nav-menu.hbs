{{#if (eq page.attributes.theme 'cheat-sheet') }}
  <aside class="toc sidebar" data-title="{{or page.attributes.toctitle ''}}"
  data-levels="{{{or page.attributes.toclevels 2}}}">
  <div class="toc-menu">
    <div class="toc-menu-placeholder"></div>
  </div>
</aside>
{{else}}
{{#with page.navigation}}
<div class="nav-panel-menu is-active" data-panel="menu">
    <nav class="nav-menu">
    {{!-- {{#unless @root.page.attributes.hide-nav-title }}
    {{#with @root.page.componentVersion}}
    <span class="title component-title"><a href="{{{relativize ./url}}}">{{./title}}</a></span>
    {{/with}}
    {{/unless}} --}}
    {{> nav-tree navigation=this}}
  </nav>
</div>
{{/with}}
{{/if}}